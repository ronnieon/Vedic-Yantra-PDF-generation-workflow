# Environment Configuration Template
# Copy this file to .envrc and add your actual API keys

# Replicate API Token (Required)
# Get yours at: https://replicate.com/account/api-tokens
export REPLICATE_API_TOKEN=your_replicate_api_token_here

# Optional: Additional AI service API keys
# ElevenLabs API Key (for text-to-speech)
# Get yours at: https://elevenlabs.io/
export ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Google Gemini API Key (for text generation)
# Get yours at: https://makersuite.google.com/app/apikey
export GEMINI_API_KEY=your_gemini_api_key_here
